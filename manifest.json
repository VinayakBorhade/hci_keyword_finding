{
	"manifest_version": 2,
	"name": "My Cool Extension",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["jquery-3.4.1.min.js", "content.js"]
		}
	],
	"browser_action" :   { 
		"default_icon" :   "images/get_started128.png" 
	},
	"background" :   { 
		"scripts" :   [ "background.js" ] 
	}
}